{% extends 'base.html.twig' %}

{% block title %}Travelli{% endblock %}

{% block body %}


    <div class="conteiner-fluid z-0">
        <div class="row m-5 row-cols-1 text-justify row-cols-xl-4 row-cols-lg-3 row-cols-sm-1 row-cols-md-2">
            {% for objekt in objekt %}
                {% if objekt.published %}
                <div class="col p-3">
                    <div class="card border-0 h-100">
                        <img src="img/{{ objekt.image }}" class="card-img-top radius" alt="...">
                        <div class="card-img-overlay">
                            <p class="card-title text-white h4 fw-bold p-3 ">{{objekt.name}}a</p>
                        </div>

                        <div class="card-body">
                            <p class="card-text mb-2">
                                {% if objekt.rate|round!=0 %}
                                    {% for i in 1..objekt.rate|round %}
                                         <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi mb-1 bi-star-fill text-warning" viewBox="0 0 16 16">
                                            <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z"/>
                                        </svg>
                                    {% endfor %}
                                {% endif %}
                                {% if objekt.rate|round!=5 %}
                                    {% for i in 1..5-objekt.rate|round %}
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi mb-1 bi-star-fill text-secondary" viewBox="0 0 16 16">
                                            <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z"/>
                                        </svg>
                                    {% endfor %}
                                {% endif %}
                                <span class="text-secondary ps-2">{{ objekt.rate }}</span>
                            </p>
                            <p class="card-text mb-2">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi text-seccondary mb-1 bi-clock" viewBox="0 0 16 16">
                                    <path d="M8 3.5a.5.5 0 0 0-1 0V9a.5.5 0 0 0 .252.434l3.5 2a.5.5 0 0 0 .496-.868L8 8.71z"/>
                                    <path d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16m7-8A7 7 0 1 1 1 8a7 7 0 0 1 14 0"/>
                                </svg>
                                <span class="fw-bold text-success ps-2">Otwarte</span>
                                <span class="fw-bold ps-2">{{ objekt.openhourmonday|date('H:i') }} - {{ objekt.closehourmonday|date('H:i') }}</span>
                            </p>
                            <p class="card-text">{{ objekt.description|u.truncate(180, '...') }}</p>
                            <p class="card-text"><small class="text-body-secondary">Ostatnio edytowano {{ objekt.updatedate|date('d-m-Y') }}</small></p>
                        </div>
                    </div>
                </div>
                {% endif %}
